<%= render 'error_messages', target: @ride %>
<%= form_with model: @ride do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :date %>
  <%= form.text_field :date %>
  <%= form.hidden_field :user_id, value: current_user.id %>
  <%= form.submit 'Submit' %>
<% end %>
